{
  "name": "cloudflare-ddns-js",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "devDependencies": {
    "@types/bun": "latest"
  },
  "scripts": {
    "build": "bun build ./index.ts --outdir ./ --target bun --env disable --minify",
    "dev": "bun run build --watch ./index.ts",
    "start": "bun index.js --env ./.env"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "cloudflare": "^4.2.0"
  }
}